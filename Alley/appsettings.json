{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Protos": {
        "Localization": "Protos" 
    },
    "Dashboard": 
    {
        "Path": "ClientApp"
    },
    "Metrics" : {
        "BaseUrl" : "http://prometheus:9090",
        "BasePath": "api/v1/query",
        "BaseQuery": "query",
        "CpuUsage": {
            "Query": "dotnet_total_memory_bytes{job!~\"docker|prometheus\"}"
        },
        "HealthCheck": {
            "Query": "up{job!~\"docker|prometheus\"}"
        }
    },
    "Services" : {
        "Exclude": [
            "docker",
            "prometheus"
        ],
        "DefaultPort": 80
    }
}